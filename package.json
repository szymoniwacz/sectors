    "@babel/plugin-syntax-jsx": "^7.24.7",
    "@babel/preset-env": "7.16.0",
    "@babel/preset-react": "^7.24.7",
    "@popperjs/core": "^2.11.8",
    "@rails/webpacker": "5.4.4",
    "axios": "^1.7.2",
    "babel-loader": "8.2.3",
